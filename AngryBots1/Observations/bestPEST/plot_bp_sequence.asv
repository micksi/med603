function samples = plot_bp_sequence(Range, sequence, B)
    % Initialize
    samples = [0 -1; 1 1];
    [probs, next] = next_thresh_est(Range, samples);
    plot(Range, probs)
    hold on;
    scatter(next, max(probs));
    for i = 1:length(sequence)
        samples(i+2, :) = [next, sequence(i)];
        [probs, next] = next_thresh_est(Range, samples, min(Range), range(Range), B);
        plot(Range, probs)
        scatter(next, max(probs));
    end
    
    hold off;
end